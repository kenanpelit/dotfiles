format = """
[](fg:#1C2128 bg:none)\
$directory\
$git_branch\
$git_status\
[](fg:#1C2128 bg:none)\
$fill\
$cmd_duration\
$time
$character"""

continuation_prompt = "[ ](fg:#33658A)"

[directory]
style = "fg:#33658A bg:#1C2128"
format = "[ $path ]($style)"
truncation_length = 3
truncate_to_repo = true
fish_style_pwd_dir_length = 1

[character]
success_symbol = "[Û±û™](purple)"
error_symbol = "[Û±û™](red)"
vimcmd_symbol = "[Û±û©](green)"

[git_branch]
format = '[ $symbol$branch(:$remote_branch) ]($style)'
symbol = "Û∞ò¨ "
style = "fg:#86BBD8 bg:#1C2128"

[git_status]
format = '[$all_status$ahead_behind]($style)'
style = "fg:#F06449 bg:#1C2128"
conflicted = "‚â†"
ahead = "‚á°${count}"
behind = "‚á£${count}"
diverged = "‚áï"
untracked = "?${count}"
stashed = "‚â°"
modified = "!${count}"
staged = "+${count}"
renamed = "¬ª${count}"
deleted = "‚úò${count}"

[cmd_duration]
format = "[ Û±é´ $duration]($style)"
style = "fg:#F26419 bg:#1C2128"
